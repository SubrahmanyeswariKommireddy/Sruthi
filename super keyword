// referring method
/*
class Person
{
	int id;
	String name;
	String dept;
	Person(int id,String name)
	{
		this.id=id;
		this.name=name;
	}
	void display()
	{
		System.out.println(id+" "+name);
	}
}
class Emp extends Person
{
	Emp(int id,String name,String dept)
	{
		super(id,name); //Refer Method
		this.dept=dept;
	}
	void display()
	{
		System.out.println(id+" "+name+" "+dept);
	}
}
class SuperKeyword
{
	public static void main(String[] args)
	{
		Person tw1=new Person(111,"priyanka");
		tw1.display();
		Emp tw2=new Emp(111,"priyanka","CSE");
		tw2.display();
