class Dog_1 extends Animal_12{  
Dog_1(){  
//super();  //-->compiler implicitly add super to its child.
System.out.println("dog is created");  
}  
}  
class SuperKeyword{  
public static void main(String args[]){  
Dog_1 d=new Dog_1();  
}}
