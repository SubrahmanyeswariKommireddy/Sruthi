	//default constructor
	/*
	class Classconstructor
	{
	int id;
	String name;
	void display()
	{
	System.out.println(id+" "+name);
	}
	public static void main(String[] args)
	{
		Classconstructor ce1=new Classconstructor();
		ce1.display();
		Classconstructor ce2=new Classconstructor();
		ce2.display();
	}
	}
	*/

	//constructor overloading
	
